***
****
***
**
*
*/
public class solution
{
	public static void print(int n)
    {
        int m=(int)(n/2);
		for(int i=1;i<=m+1;i++)
		{
			for(int j=0;j<i;j++)
			{
				System.out.print('*');
			}
			System.out.println();
		}
		for(int i=m;i>0;i--)
		{
			for(int j=1;j<=i;j++)
			{
				System.out.print('*');
			}
			System.out.println();
		}
    }
}
